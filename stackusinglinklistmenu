#include<stdio.h>
#include<stdlib.h>
int main (){
    while(1){
        printf("1. push\n");
        printf("2. pop\n");
        printf("3. peek\n");
        printf("4. show\n");
        printf("5. exit\n");
        int choice;
        printf("Enter choice: ");
        scanf("%d",&choice);
        switch (choice){
            case 1:
                break;
            case 2:
                break;
            case 3:
                break;
            case 4:
                break;
            case 5:
                exit(0);
            default:
                printf("Invalid choice\n");
        }     
    }
}
