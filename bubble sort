#include<stdio.h>
int main(){
    int n;
    printf("enter size: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter array elements: ");
    for(int i=0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    printf("After sorting Array Elements Are: \n");
    for(int i=0; i<n; i++){
        printf("%d", arr[i]);
    }
    return 0;
}
